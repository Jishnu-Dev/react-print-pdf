{
  "name": "@onedoc/react-print",
  "version": "0.1.72",
  "description": "Onedoc components for JavaScript React",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./dist/*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "scripts": {
    "build": "tsup --external @onedoc/react-print",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "(cd ./docs && mintlify dev --watch)",
    "build-components": "npm run build && tsx ./docgen/index.ts"
  },
  "pre-commit": [
    "build-components"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@mdx-js/esbuild": "^3.0.0",
    "@onedoc/client": "^0.1.28",
    "@swc/core": "^1.3.107",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.18",
    "acorn": "^8.11.3",
    "acorn-typescript": "^1.4.13",
    "autoprefixer": "^10.4.17",
    "dotenv": "^16.4.1",
    "esformatter": "^0.11.3",
    "esformatter-jsx": "^8.0.1",
    "front-matter": "^4.0.2",
    "mintlify": "^4.0.122",
    "pdf2pic": "^3.1.1",
    "pre-commit": "^1.2.2",
    "react-docgen-typescript": "^2.2.2",
    "remark-frontmatter": "^5.0.0",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true
  },
  "dependencies": {
    "postcss": "^8.4.35",
    "postcss-color-functional-notation": "^6.0.4",
    "postcss-css-variables": "^0.19.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "3.3.2",
    "ts-deepmerge": "^7.0.0"
  }
}
